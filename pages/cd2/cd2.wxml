<view class="page" hover-class="none" hover-stop-propagation="false" style="width: 100%; height: 100%;">

<map id="map" longitude="120.295556" latitude="30.420219" markers="{{markers}}" scale="14" show-location
  style="width: 100%; height: 100%;" bindregionchange="bindregionchange" bindtap="bindtapMap"></map>

</view>
<view class="high">注：您将按照如下顺序进行作业,可拖拽更换顺序</view>
<view class="container">
 <view bindtap="box" class="box" >
 <view disable-scroll="true" wx:for="{{content}}" bindtouchmove="move" bindtouchstart="movestart" bindtouchend="moveend" data-index="{{item.id}}" data-main="{{mainx}}" class="main {{mainx == item.id? 'mainmove':'mainend'}}" style="left:{{start.x}}px; top:{{start.y}}px">{{item.content}}</view>
 </view>
</view>
<view class="container2">
 <view bindtap="box" class="boxno" >
  <view class="main {{mainx == item.id? 'mainmove':'mainend'}}">1</view>
  <view class="main {{mainx == item.id? 'mainmove':'mainend'}}">2</view>
  <view class="main {{mainx == item.id? 'mainmove':'mainend'}}">3</view>
  <view class="main {{mainx == item.id? 'mainmove':'mainend'}}">4</view>
  <view class="main {{mainx == item.id? 'mainmove':'mainend'}}">5</view>
